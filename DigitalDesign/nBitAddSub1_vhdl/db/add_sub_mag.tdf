RETURNS ( combout, cout, regout);

--synthesis_resources = lut 6 
SUBDESIGN add_sub_mag
( 
	add_sub	:	input;
	dataa[4..0]	:	input;
	datab[4..0]	:	input;
	result[4..0]	:	output;
) 
VARIABLE 
	add_sub_cella0 : cyclone_lcell
		WITH (
			cin_used = "true",
			lut_mask = "96E8",
			operation_mode = "arithmetic",
			sum_lutc_input = "cin"
